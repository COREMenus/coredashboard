{
  "name": "coremenus-dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node --max-old-space-size=8192 node_modules/nuxt/bin/nuxt.js",
    "build": "node --max-old-space-size=4096 node_modules/nuxt/bin/nuxt.js build",
    "start": "node --max-old-space-size=4096 node_modules/nuxt/bin/nuxt.js start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js -- --fix",
    "build-dev": "cross-env NODE_ENV=dev nuxt build",
    "prod-debug": "node --inspect node_modules/nuxt/bin/nuxt.js start"
  },
  "dependencies": {
    "@nuxtjs/auth-next": "^5.0.0-1648802546.c9880dc",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/i18n": "^7.2.2",
    "@nuxtjs/proxy": "^2.1.0",
    "@nuxtjs/toast": "^3.3.1",
    "apexcharts": "^3.37.0",
    "axios": "^0.27.2",
    "card-validator": "^8.1.1",
    "chart.js": "^3.7.1",
    "chartjs": "^0.3.24",
    "core-js": "^3.19.3",
    "csv-writer": "^1.6.0",
    "moment": "^2.29.4",
    "nuxt": "^2.15.8",
    "socket.io-client": "^4.5.1",
    "vue": "^2.6.14",
    "vue-apexcharts": "^1.6.2",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.6.1",
    "vuex": "^3.1.3",
    "vuex-map-fields": "^1.4.1",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@nuxtjs/eslint-config": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/vuetify": "^1.12.3",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.2.0",
    "prettier": "^2.5.1"
  }
}
